

@model List<employee>
<form action="/employee/addnew">

    <table>
        <tr>
            <th> <label for="first-name">First Name:</label></th>
            <th><input type="text" id="first-name" name="Fname"></th>
        </tr>
        <tr>
            <th><label for="last-name">Midle Name:</label></th>
            <th><input type="text" id="last-name" name="Mname"></th>
        </tr>
        <tr>
            <th><label for="last-name">Last Name:</label></th>
            <th><input type="text" id="last-name" name="Lname"></th>
        </tr>
        <tr>
            <th> <label for="ssn">SSN:</label></th>
            <th><input type="text" id="ssn" name="SSN"></th>
        </tr>
        <tr>
            <th><label for="salary">Salary:</label></th>
            <th> <input type="text" id="salary" name="salary"></th>
        </tr>
        <tr>
            <th> <label for="address">Address:</label></th>
            <th> <input type="text" id="address" name="address"><br></th>
        </tr>
        <tr>
            <th>
                <label for="super">SuperVisor</label>
            </th>
            <th>
                
                <select name="superid" id="super">
                    <option value="">select supervisor</option>
                    @foreach (var item in Model)
                    {
                        <option value="@item.SSN">@item.Fname @item.Lname</option>
                    }


            </th>

            </select>
        </tr>

    </table>


    <input type="submit" value="Submit">
</form>